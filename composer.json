{
    "name": "codesleeve/stapler",
    "description": "Easy file attachment management using Eloquent in Laravel 4",
    "keywords": ["laravel", "file", "upload", "S3", "AWS", "paperclip", "lpm"],
    "license": "MIT",
    "authors": [{
        "name": "Travis Bennett",
        "email": "tandrewbennett@hotmail.com"
    }],
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/iwanwan/Imagine.git"
    }],
    "require": {
        "php":                    ">=5.4",
        "imagine/imagine":        "dev-woi",
        "illuminate/foundation":  "~4"
    },
    "require-dev": {
        "mikey179/vfsStream": "1.2.0",
        "aws/aws-sdk-php": "2.4.*@dev"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "tests/TestCase.php"
        ],
        "psr-0": {
            "Codesleeve\\Stapler": "src/"
        }
    },
    "minimum-stability": "dev"
}
